{
    "includes": [
        "**/*.{md,json,Dockerfile}"
    ],
    "excludes": [
        "**/.*/",
        ".venv/"
    ],
    "markdown": {
        "textWrap": "always",
        "lineWidth": 100,
        "strongKind": "underscores"
    },
    "plugins": [
        "https://plugins.dprint.dev/dockerfile-0.3.0.wasm",
        "https://plugins.dprint.dev/markdown-0.16.3.wasm"
    ]
}
